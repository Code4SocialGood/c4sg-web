{
  "name": "c4sg-web",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "7.10.0",
    "npm": "4.6.1"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",

    "start": "node server.js",
    "start:dev:remote": "ng serve --environment=dev_remote",
    "start:dev:local": "ng serve --environment=dev_local",
    "start:staging:remote": "ng serve --aot --environment=staging_remote",
    "start:staging:local": "ng serve --aot --environment=staging_local",
    "start:prod:remote": "ng serve --aot --prod --environment=prod_remote",
    "start:prod:local": "ng serve --aot --prod --environment=prod_local",

    "build": "ng build",
    "build:prod:local": "ng build --prod --aot --environment=prod_local",
    "build:prod:heroku": "ng build --prod --aot --environment=prod_remote",

    "test": "ng test",
    "test_single_run": "ng test --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --prod --aot --environment=${C4SG_NG_ENV}"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.0",
    "@angular/cli": "1.0.0",
    "@angular/common": "^2.4.0",
